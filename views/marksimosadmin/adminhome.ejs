<% include ./include/headcssadmin.html %>


<body class="skin-blue" ng-app="marksimosadmin" ng-controller="adminHomeController">

    <header-admin class="header" currentuser="data.currentUser"></header-admin>


    <div class="wrapper row-offcanvas row-offcanvas-left">

        <menu-admin current-menu="css.leftmenu" showtab="css.menuTabShow" currentuser="data.currentUser" changemenu="changeMenu()"></menu-admin>


        <aside class="right-side" ng-cloak>

            <section class="content" ng-if="css.leftmenu==11">
                <h2 class="page-header">Dashboard coming soon !!</h2>

                <div class="row" >
                    <div class="col-md-12">

                        <div class="box box-primary">
                            <form class="form-horizontal" role="form">

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">Registered students count(all):</label>
                                    <div class="col-xs-5">
                                        <p class="form-control-static">{{data.students.length}}</p>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">Registered students count({{data.studentsCount[0].date}}):</label>
                                    <div class="col-xs-5">
                                        <p class="form-control-static">{{data.studentsCount[0].count}}</p>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">Registered students count({{data.studentsCount[1].date}}):</label>
                                    <div class="col-xs-5">
                                        <p class="form-control-static">{{data.studentsCount[1].count}}</p>
                                    </div>
                                </div>

                                <hr>

                                <div class="form-group" ng-repeat="campaign in data.campaigns">
                                    <label class="col-xs-2 control-label text-right">Campaign :</label>
                                    <div class="col-xs-2">
                                        <p class="form-control-static">{{campaign.name}}</p>
                                    </div>

                                    <label class="col-xs-2 control-label  text-right">Teams count:</label>
                                    <div class="col-xs-1">
                                        <p class="form-control-static">{{campaign.teamList.length}}</p>
                                    </div>

                                    <label class="col-xs-2 control-label  text-right">Members count:</label>
