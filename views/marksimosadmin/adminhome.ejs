                        <div class="box box-primary">
                            <form class="form-horizontal" role="form" name="searchStudentForm">
                                <div class="box-body">

                                    <text-form-input type="text" name="newStudentUsername" label="User :" placeholder="UserName" labelclass="col-sm-2" inputclass="col-sm-4" data="data.searchStudent.username" minlength="6" minlengtherrorinfo="Username must be at least 6 characters" maxlength="20" maxlengtherrorinfo="Username is a maximum of 20 characters"></text-form-input>
