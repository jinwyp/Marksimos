<% include ./include/headcssuser.html %>


<body ng-app="marksimos">

<div class="container-fluid width_big_screen" ng-controller="chartController">

    <user-header showmenu="true" showlogout="true"  current-menu="css.menu"></user-header>



    <div class="row mainbody bgbrand" ng-if="css.menu=='Home'">
        <div class="col-md-2 user_home_divider user_brand_box">

            <div class="row ">
                <h4 class="user_maincompany">Company <span class="company_a">{{data.currentStudent.companyName | limitTo:-1}}</span> </h4>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <img src="./app/css/images/avator.png">
                </div>
                <div class="col-md-7">
                    <h5>{{data.currentStudent.name}}</h5>
                    <span class="user_title">{{data.currentStudent.companyRole}}</span>
                    <span class="user_title">{{data.currentStudent.numOfTeamMember}} Team Member</span>
                </div>
            </div>
        </div>

        <div class="col-md-7 user_home_divider user_brand_box">
            <div class="row">
                <div class="col-md-2 user_brandcolumn" ng-repeat="brand in data.currentCompany.d_BrandsDecisions">
                    <span class="user_brand" ng-cloak>{{brand.d_BrandName}}</span>
                    <ul>
                        <li class="user_product" ng-cloak ng-repeat="sku in brand.d_SKUsDecisions">{{sku.d_SKUName}}-<span>{{sku.d_TargetConsumerSegment | usersegment}}</span></li>
                    </ul>
                </div>
            </div>
        </div>

        <div class="col-md-1 user_home_divider user_brand_box">
            <div class="row">
                <div class="col-md-12">
                    <span class="user_competitorsnum">{{data.currentStudent.numOfCompany}}</span>
                    <span class="user_competitors">Companies</span>
                </div>
            </div>
        </div>

        <div class="col-md-2">
            <div class="row">
                <div class="col-md-12 user_home_divider_horizontal">
                    <span class="user_clock"><strong>1h22mins left</strong> for submitting decision</span>
                </div>
            </div>
            <div class="row">
                <span class="user_makedecision">Mission: Market Share + Profit</span>
                <button type="button" class="btn user_decisionbutton" ng-click="switchHeaderMenu('Decision')">Make Decision</button>
            </div>
        </div>

    </div>

    <div class="row mainbody" ng-if="css.menu=='Home'">
        <div class="col-md-12">
            <div class="row user_home_divider_horizontal ">
                <div class="col-md-6 col-md-offset-3 ">
                    Current Period : {{data.currentStudent.currentPeriod}}
                    <img src="./app/css/images/temp1.png" alt=""/>
                </div>
            </div>


            <div class="row">
                <div class="col-md-6 col-md-offset-3 user_search_chart">
                    <div class="input-group">
                        <div class="input-group-btn">

                            <button type="button" class="btn btn-default dropdown-toggle droplist_company" data-toggle="dropdown">Company A <span class="caret"></span></button>
                            <ul class="dropdown-menu">
                                <li><a href="">Company Axxx</a></li>
                                <li><a href="">Company B</a></li>
                                <li><a href="">Company C</a></li>
                            </ul>
                        </div><!-- /btn-group -->

                        <input type="text" class="form-control" placeholder="find report or chart">
                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button"><span class="glyphicon glyphicon-search"></span></button>
                        </span>
                    </div><!-- /input-group -->
                </div>
            </div>


            <div class="row">
                <div class="col-md-3 col-md-offset-1">
                    <div class="panel panel-primary user_chart_list user_chart_list_bg2">
                        <div class="panel-heading">
                            <h3 class="panel-title">Your Company</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item" ><a href="" ng-click="clickChartMenu('A1')">{{'ReportMenuCompanyStatus' | translate}}</a> </li>
                            <li class="list-group-item list-group-item-active"><a href="" ng-click="clickChartMenu('A2')">{{'ReportMenuFinancialReport' | translate}}</a> </li>
                            <li class="list-group-item" ><a href="" ng-click="clickChartMenu('A3')">{{'ReportMenuInventoryReport' | translate}}</a> </li>
                            <li class="list-group-item list-group-item-active" ><a href="" ng-click="clickChartMenu('A4')">{{'ReportMenuProfitabilityEvolution' | translate}}</a> </li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 ">
                    <div class="panel panel-primary user_chart_list user_chart_list_bg2">
                        <div class="panel-heading">
                            <h3 class="panel-title">Your Competitor</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item"><a href="" ng-click="clickChartMenu('B1')">{{'ReportMenuMarketShare' | translate}}</a> </li>
                            <li class="list-group-item list-group-item-active"><a href="" ng-click="clickChartMenu('B2')">{{'ReportMenuCompetitorIntelligence' | translate}}</a></li>
                            <li class="list-group-item"><a href="" ng-click="clickChartMenu('B3')">{{'ReportMenuInvestmentProfits' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active"><a href="" ng-click="clickChartMenu('B4')">{{'ReportMenuMarketSalesInventory' | translate}}</a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 ">
                    <div class="panel panel-primary user_chart_list">
                        <div class="panel-heading">
                            <h3 class="panel-title">Market Landscape</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item"><a href="" ng-click="clickChartMenu('C1')">{{'ReportMenuSegmentLeaderTop5' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active"><a href="" ng-click="clickChartMenu('C2')">{{'ReportMenuPerceptionMap' | translate}}</a></li>
                            <li class="list-group-item"><a href="" ng-click="clickChartMenu('C3')">{{'ReportMenuSegmentDistributions' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active"><a href="" ng-click="clickChartMenu('C4')">{{'ReportMenuMarketEvolution' | translate}}</a></li>
                            <li class="list-group-item"><a href="" ng-click="clickChartMenu('C5')">{{'ReportMenuMarketTrends' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active"><a href="" ng-click="clickChartMenu('C6')">{{'ReportMenuMarketIndicator' | translate}}</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>




