<% include ./include/headcssuser.html %>


<body ng-app="marksimos">





<div class="container-fluid width_big_screen" prevent-backspace-navigate-back ng-class="{'dragging':css.dragReportFlag}" ng-controller="chartController">



    <user-header showmenu="true" showlogout="true"  current-menu="css.menu" current-round="data.currentStudent.currentPeriod" is-feedback-shown='css.isFeedbackShown'></user-header>





    <!----------  Home Page  ---------->
    <div class="row mainbody bgbrand" ng-cloak ng-if="css.menu=='Home'">
        <div class="col-md-2 user_home_divider user_brand_box">

            <div class="row ">
                <h4 class="user_maincompany">{{'HomePageSecondMenuBarLabelsCompany' | translate}} <span class="company_a">{{data.currentStudent.companyName | limitTo:-1}}</span> </h4>
            </div>
            <div class="row">
                <div class="col-md-5">
                    <img src="/app/css/images/avator.png">
                </div>
                <div class="col-md-7">
                    <h5>{{data.currentStudent.name}}</h5>
                    <span class="user_title">{{data.currentStudent.companyRole}}</span>
                    <span class="user_title">{{data.currentStudent.numOfTeamMember}} Team Member</span>
                </div>
            </div>
        </div>

        <div class="col-md-7 user_home_divider user_brand_box">
            <div class="row">
                <div class="col-md-2 user_brandcolumn" ng-repeat="brand in data.currentCompany.d_BrandsDecisions">
                    <span class="user_brand" ng-cloak>{{brand.d_BrandName}}</span>
                    <ul>
                        <li class="user_product" ng-cloak ng-repeat="sku in brand.d_SKUsDecisions">{{sku.d_SKUName}}-<span>{{sku.d_TargetConsumerSegment | usersegment}}</span></li>
                    </ul>
                </div>
            </div>
        </div>

        <div class="col-md-1 user_home_divider user_brand_box">
            <div class="row">
                <div class="col-md-12">
                    <span class="user_competitorsnum">{{data.currentStudent.numOfCompany}}</span>
                    <span class="user_competitors">{{'HomePageSecondMenuBarLabelsCompanies' | translate}}</span>
                </div>
            </div>
        </div>

        <div class="col-md-2">
            <div class="row">
                <div class="col-md-12 user_home_divider_horizontal">
                    <span class="user_clock"><strong>{{data.currentTime.hour}}h {{data.currentTime.minute}}mins {{data.currentTime.second}}s  {{'HomePageSecondMenuBarLabelsTimeLeft' | translate}}</strong> {{'HomePageSecondMenuBarLabelsTimeLeftForSubmit' | translate}}</span>
                </div>
            </div>
            <div class="row">
                <span class="user_makedecision">{{'HomePageSecondMenuBarLabelsMission' | translate}}</span>
                <button type="button" class="btn user_decisionbutton" ng-click="switchHeaderMenu('Decision')">{{'HomePageSecondMenuBarLabelsMakeDecision' | translate}}</button>
            </div>
        </div>

    </div>


    <div class="row mainbody" ng-cloak ng-if="css.menu=='Home'">
        <div class="col-md-12">
            <div class="row user_home_divider_horizontal ">
                <div class="col-md-9 col-md-offset-3 ">

                    <nav class="user_period_bar  pull-left">
                        <ul class="navbar-nav">
                            <li  ng-class="{'start':$first, 'end':$last, 'active': period.currentPeriod, 'past':period.pastPeriod}" ng-repeat="period in css.periods">
                                <span class="period_info_top text-center" ng-show="period.currentPeriod">{{'HomePageSecondMenuBarCurrentPeriod' | translate}} {{period.value}}</span>
                                <span class="period_info_bottom" >{{period.value}}</span>
                            </li>
                        </ul>
                    </nav>

                </div>
            </div>


            <div class="row">
                <div class="col-md-6 col-md-offset-3 user_search_chart">
                    <div class="input-group">

                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button"><span class="glyphicon glyphicon-search"></span></button>
                        </span>
                        <input type="text" class="form-control" placeholder="find report or chart" ng-model="data.reportName" ng-change="searchReport()">


                    </div><!-- /input-group -->
                </div>
            </div>


            <div class="row">
                <div class="col-md-3 col-md-offset-1">
                    <div class="panel panel-primary user_chart_list user_chart_list_bg2">
                        <div class="panel-heading">
                            <h3 class="panel-title">{{'ReportYourCompany' | translate}}</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='A1', 'spotlight_overlay2':css.currentSearchReportName[1]=='A1', 'spotlight_overlay3':css.currentSearchReportName[2]=='A1', 'spotlight_overlay4':css.currentSearchReportName[3]=='A1'}"><a href="" ng-click="clickChartMenu('A1')">{{'ReportMenuCompanyStatus' | translate}}</a> </li>
                            <li class="list-group-item list-group-item-active" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='A2', 'spotlight_overlay2':css.currentSearchReportName[1]=='A2', 'spotlight_overlay3':css.currentSearchReportName[2]=='A2', 'spotlight_overlay4':css.currentSearchReportName[3]=='A2'}"><a href="" ng-click="clickChartMenu('A2')">{{'ReportMenuFinancialReport' | translate}}</a> </li>
                            <li class="list-group-item" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='A3', 'spotlight_overlay2':css.currentSearchReportName[1]=='A3', 'spotlight_overlay3':css.currentSearchReportName[2]=='A3', 'spotlight_overlay4':css.currentSearchReportName[3]=='A3'}"><a href="" ng-click="clickChartMenu('A3')">{{'ReportMenuInventoryReport' | translate}}</a> </li>
                            <li class="list-group-item list-group-item-active" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='A4', 'spotlight_overlay2':css.currentSearchReportName[1]=='A4', 'spotlight_overlay3':css.currentSearchReportName[2]=='A4', 'spotlight_overlay4':css.currentSearchReportName[3]=='A4'}" ><a href="" ng-click="clickChartMenu('A4')">{{'ReportMenuProfitabilityEvolution' | translate}}</a> </li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 ">
                    <div class="panel panel-primary user_chart_list user_chart_list_bg2">
                        <div class="panel-heading">
                            <h3 class="panel-title">{{'ReportYourCompetitors' | translate}}</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='B1', 'spotlight_overlay2':css.currentSearchReportName[1]=='B1', 'spotlight_overlay3':css.currentSearchReportName[2]=='B1', 'spotlight_overlay4':css.currentSearchReportName[3]=='B1'}"><a href="" ng-click="clickChartMenu('B1')">{{'ReportMenuMarketShare' | translate}}</a> </li>
                            <li class="list-group-item list-group-item-active" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='B2', 'spotlight_overlay2':css.currentSearchReportName[1]=='B2', 'spotlight_overlay3':css.currentSearchReportName[2]=='B2', 'spotlight_overlay4':css.currentSearchReportName[3]=='B2'}"><a href="" ng-click="clickChartMenu('B2')">{{'ReportMenuCompetitorIntelligence' | translate}}</a></li>
                            <li class="list-group-item" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='B3', 'spotlight_overlay2':css.currentSearchReportName[1]=='B3', 'spotlight_overlay3':css.currentSearchReportName[2]=='B3', 'spotlight_overlay4':css.currentSearchReportName[3]=='B3'}"><a href="" ng-click="clickChartMenu('B3')">{{'ReportMenuInvestmentProfits' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active" ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='B4', 'spotlight_overlay2':css.currentSearchReportName[1]=='B4', 'spotlight_overlay3':css.currentSearchReportName[2]=='B4', 'spotlight_overlay4':css.currentSearchReportName[3]=='B4'}"><a href="" ng-click="clickChartMenu('B4')">{{'ReportMenuMarketSalesInventory' | translate}}</a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-3 ">
                    <div class="panel panel-primary user_chart_list">
                        <div class="panel-heading">
                            <h3 class="panel-title">{{'ReportMarketLandscape' | translate}}</h3>
                        </div>

                        <!-- List group -->
                        <ul class="list-group">
                            <li class="list-group-item"  ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='C1', 'spotlight_overlay2':css.currentSearchReportName[1]=='C1', 'spotlight_overlay3':css.currentSearchReportName[2]=='C1', 'spotlight_overlay4':css.currentSearchReportName[3]=='C1'}"><a href="" ng-click="clickChartMenu('C1')">{{'ReportMenuSegmentLeaderTop5' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active"  ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='C2', 'spotlight_overlay2':css.currentSearchReportName[1]=='C2', 'spotlight_overlay3':css.currentSearchReportName[2]=='C2', 'spotlight_overlay4':css.currentSearchReportName[3]=='C2'}"><a href="" ng-click="clickChartMenu('C2')">{{'ReportMenuPerceptionMap' | translate}}</a></li>
                            <li class="list-group-item"  ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='C3', 'spotlight_overlay2':css.currentSearchReportName[1]=='C3', 'spotlight_overlay3':css.currentSearchReportName[2]=='C3', 'spotlight_overlay4':css.currentSearchReportName[3]=='C3'}"><a href="" ng-click="clickChartMenu('C3')">{{'ReportMenuSegmentDistributions' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active"  ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='C4', 'spotlight_overlay2':css.currentSearchReportName[1]=='C4', 'spotlight_overlay3':css.currentSearchReportName[2]=='C4', 'spotlight_overlay4':css.currentSearchReportName[3]=='C4'}"><a href="" ng-click="clickChartMenu('C4')">{{'ReportMenuMarketEvolution' | translate}}</a></li>
                            <li class="list-group-item"  ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='C5', 'spotlight_overlay2':css.currentSearchReportName[1]=='C5', 'spotlight_overlay3':css.currentSearchReportName[2]=='C5', 'spotlight_overlay4':css.currentSearchReportName[3]=='C5'}"><a href="" ng-click="clickChartMenu('C5')">{{'ReportMenuMarketTrends' | translate}}</a></li>
                            <li class="list-group-item list-group-item-active"  ng-class="{'spotlight_overlay':css.currentSearchReportName[0]=='C6', 'spotlight_overlay2':css.currentSearchReportName[1]=='C6', 'spotlight_overlay3':css.currentSearchReportName[2]=='C6', 'spotlight_overlay4':css.currentSearchReportName[3]=='C6'}"><a href="" ng-click="clickChartMenu('C6')">{{'ReportMenuMarketIndicator' | translate}}</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!----------  End Home Page  ---------->





    <!----------  Report Page  ---------->
    <div class="row mainbody bgbrand" ng-cloak ng-if="css.menu=='Report'">
        <div class="col-md-2 user_home_divider user_chart_box">

            <div class="row ">
                <h4 class="user_maincompany user_chart_company">{{'HomePageSecondMenuBarLabelsCompany' | translate}} <span class="company_a">{{data.currentStudent.companyName | limitTo:-1}}</span> </h4>

            </div>

        </div>

        <div class="col-md-5 user_home_divider user_chart_box" >
            <div class="row">
                <div class="col-md-12 user_chart_search_box">
                    <div class="input-group">

                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button"><span class="glyphicon glyphicon-search"></span></button>
                        </span>
                        <input type="text" class="form-control" placeholder="find report or chart" ng-model="data.reportName" ng-change="searchReport()">
                    </div><!-- /input-group -->
                </div>

            </div>
        </div>

        <div class="col-md-1 user_home_divider user_chart_box">
            <div class="row">
                <div class="col-md-12 " ng-click="showComparisonPage()">
                    <span class="user_chart_vs"> {{'HomePageSecondMenuBarCompareData' | translate}}</span>
                </div>
            </div>
        </div>

        <div class="col-md-2 user_home_divider user_chart_box">
            <div class="row">
                <div class="col-md-12 ">
                    <span class="user_clock user_chart_clock"><strong>{{data.currentTime.hour}}h {{data.currentTime.minute}}mins {{data.currentTime.second}}s {{'HomePageSecondMenuBarLabelsTimeLeft' | translate}}</strong> {{'HomePageSecondMenuBarLabelsTimeLeftForSubmit' | translate}}</span>
                </div>
            </div>
        </div>

        <div class="col-md-2">
            <div class="row">
                <div class="col-md-12 ">
                    <span class="user_chart_make_decision">{{'HomePageSecondMenuBarLabelsMission' | translate}}</span>
                    <button type="button" class="btn user_chart_decision_button" ng-click="switchHeaderMenu('Decision')">{{'HomePageSecondMenuBarLabelsMakeDecision' | translate}}</button>
                </div>
            </div>

        </div>

    </div>
    <!----------  End Report Top Part Page  ---------->


    <div class="row mainbody bgbrand" ng-cloak ng-if="css.menu=='Report' && css.comparisonPage">
        <div class="col-md-12">

            <div class="row all_comparison_reports">
                <div class="col-md-12">
                    <h6><span class="glyphicon glyphicon-book"></span><span class="title">All Reports</span> </h6>

                    <ul class="list-group">

                        <li class="list-group-item" ng-mousedown="startDragReport('reportA1', $event)"><span>{{'ReportMenuCompanyStatus' | translate}}</span></li>
                        <li class="list-group-item" ng-mousedown="startDragReport('reportA2', $event)">{{'ReportMenuFinancialReport' | translate}}</li>
                        <li class="list-group-item" ng-mousedown="startDragReport('reportA4', $event)">{{'ReportMenuProfitabilityEvolution' | translate}}</li>

                        <li class="list-group-item" ng-mousedown="startDragReport('reportB2', $event)">{{'ReportMenuCompetitorIntelligence' | translate}}</li>

                        <li class="list-group-item" ng-mousedown="startDragReport('reportC3', $event)">{{'ReportMenuSegmentDistributions' | translate}}</li>
                        <li class="list-group-item" ng-mousedown="startDragReport('reportC5', $event)">{{'ReportMenuMarketTrends' | translate}}</li>
                        <li class="list-group-item" ng-mousedown="startDragReport('reportC6', $event)">{{'ReportMenuMarketIndicator' | translate}}</li>

                    </ul>

                    <div class="dragReportMovingBox" ng-style="css.dragReportPosition" ng-show="css.dragReportFlag">
                    </div>

                </div>
            </div>

            <div class="row">
                <div class="col-md-6 comparison_box" ng-mouseenter="enterComparisonBox('Left')" ng-mouseleave="enterComparisonBox('')">
                    <div class="row" ng-hide="css.dragHaveLeftReport">
                        <div class="col-md-12" >
                            <p class="text-center center-block blank_title">VS</p>
                            <p class="text-center center-block blank_info">Drag your favorite report here to compare data </p>
                        </div>
                    </div>

                    <div class="row" id="comparisonBoxLeft" >

                        <div class="col-md-12 user_chart_right" ng-if="css.dragHaveLeftReport=='reportA1'">
                            <div class="row">
                                <div class="col-md-12 user_table_single">
                                    <h5>{{'ReportMenuCompanyStatus' | translate}}

                                        <div class="pull-right">
                                            <div class="btn-group">
                                                <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                                                    {{data.tableA1CompanyStatus.currentCompany.companyName}} <span class="caret"></span>
                                                </button>
                                                <ul class="dropdown-menu" role="menu">
                                                    <li ng-repeat="company in data.tableA1CompanyStatus.allCompanyData"><a href="" ng-click="switchTableReportCompany(company)">{{company.companyName}}</a></li>
                                                </ul>
                                            </div>


                                            <div class="btn-group btn-group-sm">
                                                <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='SKU'}" ng-click="css.tableReportTab='SKU' ">SKU</button>
                                                <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Brand'}" ng-click="css.tableReportTab='Brand' ">Brand</button>
                                                <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Global'}" ng-click="css.tableReportTab='Global' ">Global</button>
                                            </div>
                                        </div>
                                    </h5>

                                    <div class="table_report" ng-if="css.tableReportTab=='SKU'">

                                        <ul class="nav nav-pills">
                                            <li ng-class="{'active': data.tableA1CompanyStatus.currentSKU.SKUID==sku.SKUID}" ng-repeat="sku in data.tableA1CompanyStatus.currentCompany.SKU"><a href="" ng-click="switchTableReportA1SKU(sku)">{{sku.SKUName}}</a></li>
                                        </ul>

                                        <div class="divider_horizontal"></div>
                                        <div table-report-company-status-sku data="data.tableA1CompanyStatus.currentSKU.data"></div>


                                    </div>

                                    <div class="table_report" ng-if="css.tableReportTab=='Brand'">

                                        <ul class="nav nav-pills">
                                            <li ng-class="{'active': data.tableA1CompanyStatus.currentBrand.brandId==brand.brandId}" ng-repeat="brand in data.tableA1CompanyStatus.currentCompany.brand"><a href="" ng-click="switchTableReportA1Brand(brand)">{{brand.brandName}}</a></li>
                                        </ul>

                                        <div class="divider_horizontal"></div>
                                        <div table-report-company-status-brand data="data.tableA1CompanyStatus.currentBrand.data"></div>

                                    </div>

                                    <div class="table_report" ng-if="css.tableReportTab=='Global'">
                                        <div table-report-company-status-global data="data.tableA1CompanyStatus.currentGlobal"></div>
                                    </div>

                                </div>
                            </div>
                        </div>


                        <div class="col-md-12 user_chart_right" ng-if="css.dragHaveLeftReport=='reportA2'">
                            <div class="row">
                                <div class="col-md-12 user_table_single">
                                    <h5>{{'ReportMenuFinancialReport' | translate}}
                                        <div class="pull-right">
                                            <div class="btn-group">
                                                <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                                                    Period {{data.tableA2FinancialData.currentPeriod.period}} <span class="caret"></span>
                                                </button>
                                                <ul class="dropdown-menu" role="menu">
                                                    <li ng-repeat="period in data.tableA2FinancialData.currentCompany.periods"><a href="" ng-click="switchTableReportPeriod(period)">Period {{period.period}}</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </h5>

                                    <div class="table_report" >

                                        <ul class="nav nav-pills">
                                            <li ng-class="{'active': data.tableA2FinancialData.currentBrand.brandId==brand.brandId}" ng-repeat="brand in data.tableA2FinancialData.currentPeriod.brands"><a href="" ng-click="switchTableReportA2Brand(brand)">{{brand.brandName}}</a></li>
                                            <li ng-class="{'active': data.tableA2FinancialData.currentBrand=='allbrand'}" ><a href="" ng-click="switchTableReportA2Brand('allbrand')">{{data.tableA2FinancialData.currentPeriod.allBrands.brandName}}</a></li>
                                        </ul>

                                        <div class="divider_horizontal"></div>
                                        <div table-report-financial-report-brand data="data.tableA2FinancialData.currentBrand.data" ng-if="data.tableA2FinancialData.currentBrand!=='allbrand'"></div>
                                        <div table-report-financial-report-all-brand data="data.tableA2FinancialData.currentPeriod.allBrands.data" ng-if="data.tableA2FinancialData.currentBrand=='allbrand'"></div>

                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="col-md-12 user_chart_right" ng-if="css.dragHaveLeftReport=='reportA4'">
                            <div class="row">
                                <div class="col-md-12 user_table_single">
                                    <h5>{{'ReportMenuProfitabilityEvolution' | translate}}
                                        <div class="pull-right">
                                            <div class="btn-group btn-group-sm">
                                                <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='SKU'}" ng-click="css.tableReportTab='SKU' ">SKU</button>
                                                <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Brand'}" ng-click="css.tableReportTab='Brand' ">Brand</button>
                                                <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Global'}" ng-click="css.tableReportTab='Global' ">Global</button>
                                            </div>
                                        </div>
                                    </h5>

                                    <div class="table_report" ng-if="css.tableReportTab=='SKU'">

                                        <ul class="nav nav-pills">
                                            <li ng-class="{'active': data.tableA4ProfitabilityEvolution.currentSKU.SKUID==sku.SKUID}" ng-repeat="sku in data.tableA4ProfitabilityEvolution.allData.SKU"><a href="" ng-click="switchTableReportA4SKU(sku)">{{sku.SKUName}}</a></li>
                                        </ul>

                                        <div class="divider_horizontal"></div>
                                        <div table-report-profitability-evolution-sku data="data.tableA4ProfitabilityEvolution.currentSKU.data"></div>

                                    </div>

                                    <div class="table_report" ng-if="css.tableReportTab=='Brand'">

                                        <ul class="nav nav-pills">
                                            <li ng-class="{'active': data.tableA4ProfitabilityEvolution.currentBrand.brandId==brand.brandId}" ng-repeat="brand in data.tableA4ProfitabilityEvolution.allData.brand"><a href="" ng-click="switchTableReportA4Brand(brand)">{{brand.brandName}}</a></li>
                                        </ul>

                                        <div class="divider_horizontal"></div>
                                        <div table-report-profitability-evolution-brand data="data.tableA4ProfitabilityEvolution.currentBrand.data"></div>

                                    </div>

                                    <div class="table_report" ng-if="css.tableReportTab=='Global'">
                                        <div table-report-profitability-evolution-global data="data.tableA4ProfitabilityEvolution.currentGlobal"></div>
                                    </div>

                                </div>

                            </div>
                        </div>


                        <div class="col-md-12 user_chart_right" ng-if="css.dragHaveLeftReport=='reportB2'">
                            <div class="row">
                                <div class="col-md-12 user_table_single">
                                    <h5>{{'ReportMenuCompetitorIntelligence' | translate}}</h5>

                                    <ul class="nav nav-pills table_multimenu">
                                        <li ng-class="{'active': css.tableReportMenu == 1}"><a href="" ng-click="switchTableMenuLevel1(1)">{{'ReportCompetitorIntelligenceTableTitleTechnology' | translate}}</a></li>
                                        <li ng-class="{'active': css.tableReportMenu == 2}"><a href="" ng-click="switchTableMenuLevel1(2)">{{'ReportCompetitorIntelligenceTableTitleMarketingSales' | translate}}</a></li>
                                        <li ng-class="{'active': css.tableReportMenu == 3}"><a href="" ng-click="switchTableMenuLevel1(3)">{{'ReportCompetitorIntelligenceTableTitleOperations' | translate}}</a></li>
                                        <li ng-class="{'active': css.tableReportMenu == 4}"><a href="" ng-click="switchTableMenuLevel1(4)">{{'ReportCompetitorIntelligenceTableTitleInvestments' | translate}}</a></li>
                                    </ul>

                                    <div class="table_report" ng-if="css.tableReportMenu == 1">
                                        <ul class="nav nav-pills">
                                            <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==1}"><a href="" ng-click="switchTableReport(1)">{{'ReportCompetitorIntelligenceTechnologyAcquiredProductionAndLogisticsEfficiency' | translate}}</a></li>
                                            <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==2}"><a href="" ng-click="switchTableReport(2)">{{'ReportCompetitorIntelligenceTechnologyAcquiredProductionPlanningFlexibility' | translate}}</a></li>
                                            <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==3}"><a href="" ng-click="switchTableReport(3)">{{'ReportCompetitorIntelligenceTechnologyAvailableTechnologyLevel' | translate}}</a></li>
                                        </ul>

                                        <div class="divider_horizontal"></div>

                                        <div table-report-competitor-intelligence data="data.tableB2CompetitorIntelligence.allData.acquiredProductionAndLogisticsEfficiency" unit="%" ng-if="data.tableB2CompetitorIntelligence.currentTable==1"></div>
                                        <div table-report-competitor-intelligence data="data.tableB2CompetitorIntelligence.allData.acquiredProductionPlanningFlexibility" unit="%" ng-if="data.tableB2CompetitorIntelligence.currentTable==2"></div>
                                        <div table-report-competitor-intelligence data="data.tableB2CompetitorIntelligence.allData.availableTechnologyLevel" unit="" ng-if="data.tableB2CompetitorIntelligence.currentTable==3"></div>
                                    </div>

                                    <div class="table_report" ng-if="css.tableReportMenu == 2">
                                        <ul class="nav nav-pills">
