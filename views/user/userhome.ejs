                            <tr>
                                <td>Market Share (value %)</td>
                                <td ng-repeat="quarter in data.tableA1CompanyStatus.currentSKU.data">{{quarter.marketShareValue | number : 2}}%</td>
                            </tr>
                            <tr>
                                <td>Market Share (volume %)</td>
                                <td ng-repeat="quarter in data.tableA1CompanyStatus.currentSKU.data">{{quarter.marketShareVolume | number : 2}}%</td>
                            </tr>
                            <tr>
                                <td>Market Sales Volume (mln std. packs)</td>
