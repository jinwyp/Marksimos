                                <td ng-repeat="quarter in data.tableA1CompanyStatus.currentSKU.data">{{quarter.appliedTechnologyIndex}}</td>
                            </tr>
                            <tr>
                                <td></td>
                                <td ng-repeat="quarter in data.tableA1CompanyStatus.currentSKU.data"></td>
                            </tr>
                            <tr>
                                <td>Market Sales Value ($ mln)</td>
